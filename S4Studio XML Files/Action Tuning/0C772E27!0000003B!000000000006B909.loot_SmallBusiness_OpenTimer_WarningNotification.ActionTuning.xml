<?xml version="1.0" encoding="utf-8"?>
<I c="LootActions" i="action" m="interactions.utils.loot" n="loot_SmallBusiness_OpenTimer_WarningNotification" s="440585">
  <L n="loot_actions">
    <V t="notification_and_dialog">
      <U n="notification_and_dialog">
        <V n="dialog" t="notification">
          <U n="notification">
            <V n="icon" t="enabled">
              <V n="enabled" t="small_business" />
            </V>
            <V n="primary_icon_response" t="enabled">
              <U n="enabled">
                <E n="ui_request">SHOW_SMALL_BUSINESS_PANEL</E>
              </U>
            </V>
            <V n="secondary_icon" t="enabled">
              <V n="enabled" t="participant" />
            </V>
            <V n="text" t="single">
              <T n="single">0x6282FB2F<!--{1.String} has been open for nearly a full day! In accordance with the bylaws of the Business Bureau, {0.SimFirstName}'s Small Business will need to close in the next hour. If {1.String} has not been closed in an hour, it will automatically close.--></T>
            </V>
            <V n="text_tokens" t="enabled">
              <U n="enabled">
                <L n="tokens">
                  <V t="participant_type" />
                  <V t="small_business" />
                </L>
              </U>
            </V>
            <V n="title" t="enabled">
              <T n="enabled">0x1D5E3E7F<!--{1.String} Must Close Soon!--></T>
            </V>
          </U>
        </V>
      </U>
    </V>
  </L>
</I>